{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [ 
    {
        "name": ".NET Core",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build net core",
        "program": "${workspaceFolder}/netcore/bin/Debug/netcoreapp3.1/CamadaUsuarioNetCore.dll",
        "args": [],
        "cwd": "${workspaceFolder}"
    },
    {
        "name": ".NET Framework",
        "type": "clr",
        "request": "launch",
        "preLaunchTask": "build netframework",
        "program": "${workspaceFolder}/netframework/bin/Debug/CamadaUsuarioNetFramework.exe",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false
    },
    {
        "name": ".NET Standard",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build netstandard",
        "program": "${workspaceFolder}/netstandard/bin/Debug/netstandard2.0/BibliotecaEmNetStandard.dll",
        "args": [],
        "cwd": "${workspaceFolder}"
    }
    ]
}